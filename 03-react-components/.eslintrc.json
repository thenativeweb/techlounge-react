{
  "extends": [
    "es/browser"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "React": true,
    "ReactDOM": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "sort-imports": "off",
    "func-style": "off",
    "react/prefer-stateless-function": "off",
    "react/destructuring-assignment": "off"
  }
}
